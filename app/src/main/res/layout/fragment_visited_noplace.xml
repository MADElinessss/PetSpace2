<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".placetogo.PlaceToGoFragment">

    <FrameLayout
        android:id="@+id/noplacetogoframelayout"
        android:layout_width="match_parent"
        android:layout_height="461dp"
        android:layout_marginTop="80dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="20dp"
            android:fontFamily="@font/bmjua"
            android:text="아직 방문한 여행일정이 없어요"
            android:textColor="@color/black"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="[우주댕댕이]와 함께 한적하고 조용한 곳에\n다녀오는 건 어떠신가요?"
            android:textColor="#0C201E"
            android:textSize="16dp" />

        <Button
            android:id="@+id/tv_search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="80dp"
            android:text="숙소 검색하기"
            android:textColor="#2662BB" />


    </FrameLayout>

</FrameLayout>